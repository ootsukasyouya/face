<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="col"> <%= f.label :email %>
  <%= f.email_field :email %></div>
  <div class="col"><%= f.label :password %>
  <%= f.password_field :password %></div>
  <div class="col"><%= f.submit "Log in" %></div>
<% end %>
